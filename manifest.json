{
  "manifest_version": 3,
  "name": "Dark Mode Websites",
  "version": "1.0.0",
    "permissions": [
    "storage" 
  ],
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "src/background/index.ts",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/content/index.ts"]
    }
  ]
}